{
  "manifest_version": 2,
  "name": "Color Changer",
  "version": "0.1.0",
  "description": "Change element colors!",
  "icons": {
    "16": "pallet.png"
  },
  "permissions": ["tabs", "http://*/*", "https://*/*"],
  "browser_action": {
    "default_title": "Change element colors!",
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [{ "matches": ["<all_urls>"] , "js": ["content.js"] }]
}
